{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "gt",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_line"
      }
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "blank_line"
        },
        {
          "type": "SYMBOL",
          "name": "node_line"
        },
        {
          "type": "SYMBOL",
          "name": "property"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "blank_line": {
      "type": "PATTERN",
      "value": "\\n"
    },
    "node_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "node"
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "node": {
      "type": "PATTERN",
      "value": "[^\\s\\n][^\\n]*"
    },
    "property": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "    "
        },
        {
          "type": "SYMBOL",
          "name": "key"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "value"
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "key": {
      "type": "PATTERN",
      "value": "[^:\\n]+"
    },
    "value": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "[ \\t]+[^\\n]*"
            },
            {
              "type": "PATTERN",
              "value": "\\n"
            }
          ]
        }
      }
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}