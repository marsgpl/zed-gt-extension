{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "gt",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "node"
          },
          {
            "type": "SYMBOL",
            "name": "property"
          },
          {
            "type": "SYMBOL",
            "name": "error_node"
          },
          {
            "type": "SYMBOL",
            "name": "invalid_indent"
          },
          {
            "type": "SYMBOL",
            "name": "blank_line"
          }
        ]
      }
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]*"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "node": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[a-zA-Z0-9_\\- ]+"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "error_node": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^\\s\\n][^\\n]*"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "property": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "    "
        },
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "key"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": " "
                },
                {
                  "type": "FIELD",
                  "name": "value",
                  "content": {
                    "type": "SYMBOL",
                    "name": "value"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "key": {
      "type": "PATTERN",
      "value": "[^:\\n]+"
    },
    "value": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "invalid_indent": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": " +[^\\n]*"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "blank_line": {
      "type": "PATTERN",
      "value": "\\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}