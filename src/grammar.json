{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "gt",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_line"
      }
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "blank_line"
        },
        {
          "type": "SYMBOL",
          "name": "valid_node"
        },
        {
          "type": "SYMBOL",
          "name": "invalid_node"
        },
        {
          "type": "SYMBOL",
          "name": "property_line"
        },
        {
          "type": "SYMBOL",
          "name": "comment_line"
        }
      ]
    },
    "blank_line": {
      "type": "PATTERN",
      "value": "\\n"
    },
    "valid_node": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "[a-zA-Z0-9_\\- ]+"
            },
            {
              "type": "PATTERN",
              "value": "\\n"
            }
          ]
        }
      }
    },
    "invalid_node": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[^\\s\\n][^\\n]*"
          },
          {
            "type": "PATTERN",
            "value": "\\n"
          }
        ]
      }
    },
    "property_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[ \\t]+"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "valid_key"
            },
            {
              "type": "SYMBOL",
              "name": "invalid_key"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": " +"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "valid_value"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "invalid_value"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "valid_key": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[a-zA-Z0-9_\\- ]+"
        }
      }
    },
    "invalid_key": {
      "type": "PATTERN",
      "value": "[^:\\n]+"
    },
    "valid_value": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[a-zA-Z0-9_\\- ]+"
        }
      }
    },
    "invalid_value": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "comment_line": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[ \\t]+[^:\\n]*"
          },
          {
            "type": "PATTERN",
            "value": "\\n"
          }
        ]
      }
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}