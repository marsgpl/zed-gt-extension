{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "gt",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "blank_line"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "valid_node"
          },
          {
            "type": "SYMBOL",
            "name": "valid_property"
          },
          {
            "type": "SYMBOL",
            "name": "invalid_line"
          }
        ]
      }
    },
    "blank_line": {
      "type": "PATTERN",
      "value": "\\n"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": "[^\\n]*"
          },
          {
            "type": "STRING",
            "value": "\n"
          }
        ]
      }
    },
    "valid_node": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[a-zA-Z0-9_\\- ]+"
          },
          {
            "type": "STRING",
            "value": "\n"
          }
        ]
      }
    },
    "valid_property": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "    "
        },
        {
          "type": "SYMBOL",
          "name": "key"
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": ":"
          }
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "value"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "key": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_\\- ]+"
    },
    "value": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_\\- ]+"
    },
    "invalid_line": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^\\n]+"
            },
            {
              "type": "STRING",
              "value": "\n"
            }
          ]
        }
      }
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}