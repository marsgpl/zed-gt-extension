{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "gt",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "node"
          },
          {
            "type": "SYMBOL",
            "name": "property"
          },
          {
            "type": "SYMBOL",
            "name": "error_node"
          },
          {
            "type": "SYMBOL",
            "name": "wrong_indent"
          },
          {
            "type": "SYMBOL",
            "name": "blank_line"
          }
        ]
      }
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]*"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "node": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[a-zA-Z0-9_\\- ]+"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "error_node": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^\\s\\n][^\\n]*"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "property": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "    "
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "property_valid"
            },
            {
              "type": "SYMBOL",
              "name": "property_invalid"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "property_valid": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "key",
            "content": {
              "type": "SYMBOL",
              "name": "valid_key"
            }
          },
          {
            "type": "STRING",
            "value": ":"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": " "
                  },
                  {
                    "type": "FIELD",
                    "name": "value",
                    "content": {
                      "type": "SYMBOL",
                      "name": "valid_value"
                    }
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "property_invalid": {
      "type": "PATTERN",
      "value": "[^\\n]*"
    },
    "valid_key": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_\\- ]+"
    },
    "valid_value": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_\\- ]+"
    },
    "wrong_indent": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": " {1,3}| {5,}"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]*"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "blank_line": {
      "type": "PATTERN",
      "value": "\\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}