{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "gt",
      "camelcase": "GT",
      "title": "Graph Text",
      "scope": "source.gt",
      "file-types": [
        "gt"
      ],
      "injection-regex": "^gt$",
      "class-name": "TreeSitterGt"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Gt grammar for tree-sitter",
    "authors": [
      {
        "name": "marsgpl",
        "email": "marsgpl@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/marsgpl/zed-gt-extension"
    }
  },
  "bindings": {
    "c": true,
    "go": false,
    "java": false,
    "node": false,
    "python": false,
    "rust": false,
    "swift": false,
    "zig": false
  }
}
